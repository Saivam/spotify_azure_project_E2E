{
	"name": "CSVConnecter",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorageConnect",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"containerName": {
				"type": "String"
			},
			"fileName": {
				"type": "String"
			},
			"folderName": {
				"type": "String"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@if(equals(length(dataset().fileName),0),'',dataset().fileName)",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().folderName",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().containerName",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "transaction_id",
				"type": "String"
			},
			{
				"name": "transactional_date",
				"type": "String"
			},
			{
				"name": "product_id",
				"type": "String"
			},
			{
				"name": "customer_id",
				"type": "String"
			},
			{
				"name": "payment",
				"type": "String"
			},
			{
				"name": "credit_card",
				"type": "String"
			},
			{
				"name": "loyalty_card",
				"type": "String"
			},
			{
				"name": "cost",
				"type": "String"
			},
			{
				"name": "quantity",
				"type": "String"
			},
			{
				"name": "price",
				"type": "String"
			}
		]
	}
}